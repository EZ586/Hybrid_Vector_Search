{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run harness (PreFilter backend)",
      "type": "shell",
      "command": "python",
      "args": [
        "-m",
        "src.harness.run",
        "--version",
        "dev",
        "--backend",
        "pre_filter",
        "--K",
        "10"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
